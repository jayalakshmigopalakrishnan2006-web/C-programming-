#include <stdio.h>

int main() {
    int steps, time;
    
    printf("Enter number of steps: ");
    scanf("%d", &steps);
    printf("Enter time taken (in minutes): ");
    scanf("%d", &time);

    if (steps >= 10000) {
        printf("Fitness Level: Excellent\n");
    } else if (steps >= 5000 && steps < 10000) {
        printf("Fitness Level: Good\n");
    } else {
        printf("Fitness Level: Sedentary\n");
    }

    return 0;
}
